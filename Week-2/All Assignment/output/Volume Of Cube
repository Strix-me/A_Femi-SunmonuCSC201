#include <iostream>
using namespace std;
main()
{
    //temperature conversion//
    int r,h;
    cout << "Please enter you height of cylinder: " ;
    cin >> h;
    cout << "Please enter you radius of cylinder: " ;
    cin >> r;

    cout << "The Area of your cylinder us is  "<< 3.142 * h * (r*r)<< ".\n";
    return 0;
} 